	/delete-property/ bias-disable;
	bias-pull-down;
};
&sdhc_2 {/*tinno modify 0910*/
	//qcom,nonremovable;
	interrupts = <0 1 2>;
	interrupt-map = <0 &intc 0 125 0
		1 &intc 0 221 0
		2 &msm_gpio 38 0>;
	interrupt-names = "hc_irq", "pwr_irq", "status_irq";

	cd-gpios = <&msm_gpio 38 0x1>;
	///delete-property/ cd-gpios;
};

